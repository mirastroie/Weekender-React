{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "settings": {
        "react": {
        "version": "detect"
        }
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc" : 0,
        "max-len": ["error", { "code": 150 }],
        "@typescript-eslint/no-unused-imports": "off"
    }
}
